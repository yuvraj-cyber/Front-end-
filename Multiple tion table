!DOCTYPE html>

<html lang="en">



<head>

    <meta charset="UTF-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Document</title>

</head>



<body>







    <script>





        const number = parseInt(prompt('Enter an integer: '));





        for (let i = 1; i <= 10; i++) {





            var result = i * number;

            document.write(`${number} * ${i} = ${result}`);

            document.write("<BR>");



        }

      

      

    </script>

</body>



</html>
